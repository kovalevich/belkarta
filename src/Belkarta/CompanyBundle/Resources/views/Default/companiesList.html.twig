<div>
    <div class="row">
    {% if items|length > 0 %}
        {% for item in items %}
            <div class="col-sm-1">
                {% if item.logo %}<img src="{{ item.logo.image.path }}" title="Логотип {{ item.name }}" class="img-responsive img-circle">{% endif %}
                <a href="#">{{ item.name }}</a>
            </div>
        {% endfor %}
    {% else %}
        По вашему запросу пока нет компаний
    {% endif %}
    </div>
    <div class="clear"></div>
</div>